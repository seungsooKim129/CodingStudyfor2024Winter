#include <iostream>
using namespace std;
int main() {
    int T, n, total=0, x; 
    cin >> T;
    for (int i = 1; i <=T; i++) {
        total = 0;
        cin >> n;
        for (int j = 1; j <= n; j++) {
            cin >> x;
            total += x;
        }
        cout << total << "\n";
    }  
}
