#include <iostream>
#include <string>
using namespace std;
int main() {
    int a, b, c, d, idx=0, total=0, max=0;      
    for (int i = 1; i <= 5; i++) {
        total = 0;
        cin >> a>>b>>c>>d;
        total += a + b + c + d;
        if (max < total) {
            max = total;
            idx = i;
        }
    }
    cout << idx << " " << max;
}
